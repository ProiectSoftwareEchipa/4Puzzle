                    <Rectangle.Fill>
                        <ImageBrush ImageSource="ms-appx:///Images/Message Box Victory.png"/>
                    </Rectangle.Fill>
                </Rectangle>
                <TextBox x:Name="PopupTextBoxUsername" Canvas.Left="64" Margin="0" TextWrapping="Wrap" Text="Register Name" Canvas.Top="296" Width="275" Canvas.ZIndex="4" GotFocus="PopupTextBoxUsername_GotFocus" Height="37" BorderBrush="Black"/>
                <TextBlock x:Name="PopupTextBlockMessage" Canvas.Left="12" TextWrapping="Wrap" Text="New highscore!" Canvas.Top="267" Canvas.ZIndex="4" FontSize="24" Height="34" Foreground="Black" FontFamily="ms-appx:/Fonts/RO-D3-Streetism.ttf#RO D3 Streetism" TextAlignment="Center" Width="376"/>
                <TextBlock x:Name="PopupTextBlockMessagePlayAgain" Canvas.Left="12" TextWrapping="Wrap" Text="Play again?" Canvas.Top="282" Canvas.ZIndex="4" FontSize="32" Height="49" Foreground="Black" FontFamily="ms-appx:/Fonts/RO-D3-Streetism.ttf#RO D3 Streetism" TextAlignment="Center" Width="376"/>
                <Button x:Name="PopupButtonCancel" Content="" Canvas.Left="205" Canvas.Top="327" Canvas.ZIndex="4" Click="buttonPopupCancel_Click" Height="62">
                    <Button.BorderBrush>
                        <ImageBrush Stretch="Fill"/>
                    </Button.BorderBrush>
                    <Button.Background>
                        <ImageBrush Stretch="Uniform" ImageSource="ms-appx:///Images/Cancel button.png"/>
                    </Button.Background>
                </Button>
                <Button x:Name="PopupButtonOk" Content="" Canvas.Left="85" Canvas.Top="328" Canvas.ZIndex="4" Click="buttonPopupOk_Click" Height="60" Width="99">
                    <Button.BorderBrush>
                        <ImageBrush Stretch="Fill"/>
                    </Button.BorderBrush>
                    <Button.Background>
                        <ImageBrush Stretch="Uniform" ImageSource="ms-appx:///Images/Ok button.png"/>
                    </Button.Background>
                </Button>
                <Image x:Name="imageTitle" Height="186" Canvas.Left="95" Canvas.Top="-43" Width="226" Source="ms-appx:///Images/medium_game.png"/>
                <Image x:Name="imageHeaderBackground" Height="82" Canvas.Left="25" Canvas.Top="90" Width="353" Source="ms-appx:///Images/Notebook background.png"/>
